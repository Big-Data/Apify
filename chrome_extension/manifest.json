{
  "manifest_version": 2,
  "name": "Apify",
  "description": "Turn any website into an API.",
  "version": "1.0",
  "background": {
    "scripts":["background.js"]
  },
  "permissions": [
    "contextMenus"
  ],
  "content_scripts": [
    {
        "matches":["<all_urls>"],
        "js":["content.js"]
    }
  ]
}
